
//consutruimos la imagen del docker file


 docker build --tag=pandoracentermicroservices:latest .
 
 
 run 
 
 
docker container run --network minet pandoracentermicroservices:latest


select *from pandoradb.poscheckinfa as u where status='ACCEPT_FARMER' inner join pandoradb.pandoracheckfarmer as ar ON ar.id=u.pandora_check_id;
select * from pandoradb.poscheckinfar as a INNER JOIN pandoradb.pandoracheckfarmer as b  ON a.pandora_check_id = b.id WHERE a.status='ACCEPT_FACTORY' AND b.transacc_id: '13b8ab97-8e89-42c9-9f4f-aa7c1f2a184e';
